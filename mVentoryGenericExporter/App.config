<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <connectionStrings>
    <clear/>
    <add name="DBSource" providerName="System.Data.SqlClient" connectionString="Persist Security Info=False;Initial Catalog=Chances;Data Source=.;Integrated Security=SSPI"/>
  </connectionStrings>
  <appSettings>
   <add key="LogFile" value="mvExporterLog.txt"/>
  </appSettings>
</configuration>

<!-- == Config file documentation ==

* LogFile - name of the log file without the path. It will be created in the same folder as the executable.

-->
